{
  "name": "securli-node",
  "version": "1.0.0",
  "description": "securily Project:",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "start": "env-cmd -e default ts-node src/index.ts",
    "start:local": "env-cmd -e local nodemon src/index.ts",
    "prod": "tsc && node ./build/index.js",
    "migration:generate": "env-cmd -e default ts-node ./node_modules/typeorm/cli.js migration:generate -d src/ormconfig.ts",
    "migration:up": "env-cmd -e default ts-node ./node_modules/typeorm/cli.js migration:run -d src/ormconfig.ts",
    "migration:down": "env-cmd -e default ts-node ./node_modules/typeorm/cli.js migration:revert -d src/ormconfig.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sparkparker/node-securily.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sparkparker/node-securily/issues"
  },
  "homepage": "https://github.com/sparkparker/node-securily#readme",
  "dependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@sendgrid/mail": "^7.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/helmet": "^4.0.0",
    "aws-sdk": "^2.1340.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cron": "^2.3.0",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "get-stream": "^6.0.1",
    "jade": "^1.11.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "node-html-parser": "^6.1.4",
    "nodemailer": "^6.8.0",
    "ssh2-sftp-client": "^8.1.0",
    "swagger-jsdoc": "^6.2.7",
    "swagger-ui-express": "^4.6.0",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.11",
    "typescript": "^4.9.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.11.17",
    "@types/ssh2-sftp-client": "^9.0.0",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
